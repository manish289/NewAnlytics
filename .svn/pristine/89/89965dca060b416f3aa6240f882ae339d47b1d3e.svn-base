package com.isource.dto.companyprofile;

import java.math.BigDecimal;

import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.AllArgsConstructor;
import lombok.NoArgsConstructor;
import lombok.ToString;


@AllArgsConstructor
@NoArgsConstructor
@ToString
public class StatisticsDto {

	@JsonProperty("participated_result_count")
	private int participated_result_count;// bigint

	@JsonProperty("participated_contract_value")
	private BigDecimal participated_contract_value;// numeric

	@JsonProperty("awarded_tender")
	private int awarded_tender;// bigint

	@JsonProperty("awarded_tender_value")
	private BigDecimal awarded_tender_value;// numeric

	@JsonProperty("lost_tender")
	private int lost_tender;// bigint

	@JsonProperty("lost_tender_value")
	private BigDecimal lost_tender_value;// numeric

	@JsonProperty("result_to_be_awarded")
	private int result_to_be_awarded;// bigint

	@JsonProperty("result_to_be_awarded_value")
	private BigDecimal result_to_be_awarded_value;// numeric

	public int getParticipated_result_count() {
		return participated_result_count;
	}

	public void setParticipated_result_count(int participated_result_count) {
		this.participated_result_count = participated_result_count;
	}

	public BigDecimal getParticipated_contract_value() {
		return participated_contract_value==null?new BigDecimal(0):participated_contract_value;
	}

	public void setParticipated_contract_value(BigDecimal participated_contract_value) {
		this.participated_contract_value = participated_contract_value;
	}

	public int getAwarded_tender() {
		return awarded_tender;
	}

	public void setAwarded_tender(int awarded_tender) {
		this.awarded_tender = awarded_tender;
	}

	public BigDecimal getAwarded_tender_value() {
		return awarded_tender_value==null?new BigDecimal(0):awarded_tender_value;
	}

	public void setAwarded_tender_value(BigDecimal awarded_tender_value) {
		this.awarded_tender_value = awarded_tender_value;
	}

	public int getLost_tender() {
		return lost_tender;
	}

	public void setLost_tender(int lost_tender) {
		this.lost_tender = lost_tender;
	}

	public BigDecimal getLost_tender_value() {
		return lost_tender_value==null?new BigDecimal(0):lost_tender_value;
	}

	public void setLost_tender_value(BigDecimal lost_tender_value) {
		this.lost_tender_value = lost_tender_value;
	}

	public int getResult_to_be_awarded() {
		return result_to_be_awarded;
	}

	public void setResult_to_be_awarded(int result_to_be_awarded) {
		this.result_to_be_awarded = result_to_be_awarded;
	}

	public BigDecimal getResult_to_be_awarded_value() {
		return result_to_be_awarded_value==null?new BigDecimal(0):result_to_be_awarded_value;
	}

	public void setResult_to_be_awarded_value(BigDecimal result_to_be_awarded_value) {
		this.result_to_be_awarded_value = result_to_be_awarded_value;
	}
	
}
